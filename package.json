    "per-file": true,
    "all": true,
    "include": [
      "src/**/*.*"
    ],
    "exclude": [
      "src/main.js",
      "src/router.js"
    ],
    "extension": [
      ".js",
      ".ts",
      ".vue"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "cache": true
  }
