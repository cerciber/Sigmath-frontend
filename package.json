    "view-doc": "start chrome ./docs/index.html",
    "start": "node app.js",
    "test": "mochapack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=text --reporter=text-summary --reporter=lcov npm run test",
    "view-cover": "start chrome coverage/lcov-report/index.html"
  },
